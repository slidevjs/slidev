    "@antfu/utils": "^0.1.4",
    "@iconify/json": "^1.1.338",
    "@slidev/client": "workspace:*",
    "@slidev/parser": "workspace:*",
    "@slidev/types": "workspace:*",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "cli-progress": "^3.9.0",
    "codemirror": "^5.61.0",
    "connect": "^3.7.0",
    "fast-glob": "^3.2.5",
