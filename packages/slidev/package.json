    "js-base64": "^3.6.0",
    "js-yaml": "^3.14.1",
    "kolorist": "^1.4.1",
    "markdown-it": "^12.0.6",
    "markdown-it-link-attributes": "^3.0.0",
    "monaco-editor": "^0.23.0",
    "pdf-lib": "^1.16.0",
    "prismjs": "^1.23.0",
    "prompts": "^2.4.1",
    "shiki": "^0.9.3",
    "sirv": "^1.0.11",
    "vite": "^2.2.4",
    "vite-plugin-components": "^0.8.4",
